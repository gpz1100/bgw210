# mount
/dev/root on / type squashfs (ro,relatime)
devtmpfs on /dev type devtmpfs (rw,relatime,size=242692k,nr_inodes=60673,mode=755)
proc on /proc type proc (rw,relatime)
tmpfs on /var type tmpfs (rw,relatime,size=420k)
tmpfs on /mnt type tmpfs (rw,relatime,size=16k,mode=755)
sysfs on /sys type sysfs (rw,relatime)
tmpfs on /run type tmpfs (rw,relatime,size=16k,mode=755)
debugfs on /sys/kernel/debug type debugfs (rw,relatime)
devpts on /dev/pts type devpts (rw,relatime,mode=600,ptmxmode=000)
ubi:data on /data type ubifs (rw,relatime,compr=lzo,assert=read-only,ubi=0,vol=10)
ubi:defaults on /mnt/defaults type ubifs (ro,relatime,assert=read-only,ubi=0,vol=11)
cgroup on /sys/fs/cgroup type tmpfs (rw,relatime,mode=755)
cgroup on /sys/fs/cgroup/cpuset type cgroup (rw,relatime,cpuset)
cgroup on /sys/fs/cgroup/cpu type cgroup (rw,relatime,cpu)
cgroup on /sys/fs/cgroup/cpuacct type cgroup (rw,relatime,cpuacct)
cgroup on /sys/fs/cgroup/blkio type cgroup (rw,relatime,blkio)
cgroup on /sys/fs/cgroup/memory type cgroup (rw,relatime,memory)
cgroup on /sys/fs/cgroup/devices type cgroup (rw,relatime,devices)
cgroup on /sys/fs/cgroup/freezer type cgroup (rw,relatime,freezer)
tmpfs on /media type tmpfs (rw,relatime)
tmpfs on /var type tmpfs (rw,relatime)
devfs on /dev type tmpfs (rw,relatime)
pts on /dev/pts type devpts (rw,relatime,mode=600,ptmxmode=000)
ubi:data on /var/lib/docker/overlay2 type ubifs (rw,relatime,compr=lzo,assert=read-only,ubi=0,vol=10)
ubi:data on /var/lib/docker/image type ubifs (rw,relatime,compr=lzo,assert=read-only,ubi=0,vol=10)
/dev/loop0 on /data/dockerfs/root type ext4 (rw,relatime)
/dev/loop0 on /var/lib/docker/containers type ext4 (rw,relatime)
/dev/loop0 on /var/lib/docker/trust type ext4 (rw,relatime)
/dev/loop0 on /var/lib/docker/volumes type ext4 (rw,relatime)
/dev/loop0 on /var/lib/docker/network type ext4 (rw,relatime)
tmpfs on /var/dpi type tmpfs (rw,relatime)
